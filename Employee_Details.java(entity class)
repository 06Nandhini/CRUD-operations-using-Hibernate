package com.example;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;

@Entity
// @Table(name= Employee_details) --> If you want to enter the table name explicitly
public class Employee_details {
    @Id
    private int id;
    private String name;
    private float salary;
    public Employee_details(){

    }
    public Employee_details(int id,String name,float s){
        this.id=id;
        this.name=name;
        this.salary=s;
    }
    public void setName(String n){ this.name=n;}
    public void setId(int id){ this.id=id;}
    public void setSalary(float s){ this.salary=s;}
    public String getName(){ return name; }
    public float getSalary(){ return salary; }
    public int getId(){ return id; }

    public String toString(){
        return ("Employee Details\nName : "+this.name+"\nId : "+this.id+"\nSalary : "+this.salary);
    }
}
